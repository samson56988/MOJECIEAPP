
@{
    ViewBag.Title = "Upload";
}


<div class="page-wrapper mdc-toolbar-fixed-adjust">
    <main class="content-wrapper">
        <div class="mdc-layout-grid">
            <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell--span-12">
                    <div class="mdc-card">
                        <h6 class="card-title">Upload KYC File</h6>
                        <div class="template-demo">
                            <div class="mdc-layout-grid__inner">
                                <form class="form" method="post" enctype="multipart/form-data">
                                    <br />
                                    <div class="file-upload-wrapper" data-text="Select your file!">
                                        <input name="file" type="file" required />
                                    </div>
                                    @ViewBag.Result
                                    @ViewBag.Error
                                    <br />
                                    <div>
                                        <button type="submit" class="mdc-button mdc-button--outlined mdc-ripple-upgraded" style="--mdc-ripple-fg-size:57px; --mdc-ripple-fg-scale:1.96763; --mdc-ripple-fg-translate-start:39.5px, 0.5px; --mdc-ripple-fg-translate-end:19.3008px, -10.5px;">
                                            Upload
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </main>
</div>