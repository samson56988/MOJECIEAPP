
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



            <div class="page-wrapper mdc-toolbar-fixed-adjust">
                <main class="content-wrapper">
                    <div class="mdc-layout-grid">
                        <div class="mdc-layout-grid__inner">
                            <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet">
                                <div class="mdc-card info-card info-card--success">
                                    <div class="card-inner">
                                        <h5 class="card-title">Shomolu</h5>
                                        <h5 class="font-weight-light pb-2 mb-1 border-bottom">@ViewBag.Shomolu</h5>
                                        <p class="tx-12 text-muted">KYC</p>
                                        <div class="card-icon-wrapper">
                                            <i class="material-icons">dvr</i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet">
                                <div class="mdc-card info-card info-card--danger">
                                    <div class="card-inner">
                                        <h5 class="card-title">Ikeja</h5>
                                        <h5 class="font-weight-light pb-2 mb-1 border-bottom">@ViewBag.Ikeja</h5>
                                        <p class="tx-12 text-muted">KYC</p>
                                        <div class="card-icon-wrapper">
                                            <i class="material-icons">dvr</i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-4-desktop mdc-layout-grid__cell--span-4-tablet">
                                <div class="mdc-card info-card info-card--primary">
                                    <div class="card-inner">
                                        <h5 class="card-title">Oshodi</h5>
                                        <h5 class="font-weight-light pb-2 mb-1 border-bottom">@ViewBag.Oshodi</h5>
                                        <p class="tx-12 text-muted">KYC</p>
                                        <div class="card-icon-wrapper">
                                            <i class="material-icons">dvr</i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            
                          
                <!-- partial -->
            </div>
                        </div>
                    </main>
                </div>
      
